<section class="main-content__body main-content__body--flush">
  <table>
    <thead>
      <th>ID</th>
      <th>注文合計金額</th>
      <th>注文日</th>
    </thead>
    <tbody>
      <% @shippings.each do |shipping| %>
        <tr>
          <td><%= shipping.id %></td>
          <td><%= number_to_currency(shipping.order.total_price, unit: "￥", precision: 0) %></td>
          <td><%= I18n.l(shipping.created_at, format: :long) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</section>